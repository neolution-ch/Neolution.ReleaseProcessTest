{
  "git": {
    "commitMessage": "chore: release v${version}",
    "tagName": "v${version}",
    "push": true,
    "commit": true
  },
  "github": {
    "release": true,
    "releaseName": "Release ${version}",
    "tokenRef": "GITHUB_TOKEN",
    "preRelease": false
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md",
      "strictLatest": false,
      "addUnreleased": true
    }
  }
}